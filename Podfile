source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '9.0'

target 'UnitTestIssue' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  use_frameworks!

  pod 'AFNetworking'
  pod "PromiseKit", "~> 6.0"

  target 'UnitTestIssueTests' do
    inherit! :search_paths

    pod 'OCHamcrest', '~> 7.1'
    pod 'OCMockito', '~> 5.0'

  end

end
